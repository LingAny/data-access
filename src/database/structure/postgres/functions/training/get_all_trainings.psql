CREATE OR REPLACE FUNCTION get_training_by_id()

RETURNS TABLE(training_id UUID,
              category_id UUID,
              native_word TEXT,
              foreign_word TEXT)
AS
$BODY$
  BEGIN
    RETURN QUERY
    SELECT training_id, category_id, native_word, foreign_word
    FROM training;
  END;
$BODY$
LANGUAGE plpgsql VOLATILE;
